	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script>
		function isTracking(yes) {
			var trackBtn = document.getElementById('trackBtn');

			if (yes) {
				// When tracking is in progress
				trackBtn.disabled = true;
				trackBtn.style.cursor = 'not-allowed';
				trackBtn.style.opacity = '0.7';
				trackBtn.value = 'Checking...';
				trackBtn.style.fontStyle = 'italic';
			} else {
				// When tracking is not in progress
				trackBtn.disabled = false;
				trackBtn.style.cursor = 'pointer';
				trackBtn.style.opacity = '1';
				trackBtn.value = 'Check Booking';
				trackBtn.style.fontStyle = 'normal';
			}
		}

		function handleTrack(e) {
			e.preventDefault();
			isTracking(true)

			// Retrieve the tracking ID from the input field
			var trackingId = document.getElementById('trackingId').value;

			//console.log("handleTrack: ", trackingId)

			// Validate that the tracking ID is not empty
			if (trackingId.trim() === "") {
				Swal.fire({
					icon: "error",
					title: "Booking Error",
					text: "Please enter your booking reference."
				});
				isTracking(false)
				return;
			}

			// If the tracking ID is not empty, construct the URL
			var apiUrl = 'https://flight-backend-n3cr.onrender.com/api/flights/'+trackingId.trim().toUpperCase();

			// Use fetch to send the GET request
			fetch(apiUrl)
				.then(response => {
					if (!response.ok) {
						// If the response is not OK, read the response body
						return response.json().then(errorData => {
							throw new Error(errorData?.error || "A server error occurred. Please try again later.");
						});
					}
					return response.json();
				})
				.then(response => {
					// You can use the data for further processing or display on the page
					// For example, update the UI with the fetched data
					
					updateUIWithData(response.data, trackingId);
				})
				.catch(error => {
					//console.log("error:::", error)
					Swal.fire({
						icon: "error",
						title: "Tracking Error",
						text: error.message
					});
				})
				.finally(() => {
					isTracking(false)
				})
		}

		function fbTimeToDate(date) {
			const seconds = date._seconds
			const nanoseconds = date._nanoseconds
			const milliseconds = seconds * 1000

			date = new Date();
			date.setTime(milliseconds)

			return date;
		}

		function formatDate(date, dontConvertDate) {
			if (!dontConvertDate) date = fbTimeToDate(date)

			const day = date.getDate();
			const monthIndex = date.getMonth();
			const year = date.getFullYear();

			const monthNames = [
				"Jan", "Feb", "Mar", "Apr", "May", "Jun",
				"Jul", "Aug", "Sep", "Oct", "Nov", "Dec"
			];

			return `${day} ${monthNames[monthIndex]} ${year}`;
		}

		const getFullname = (data, separator) => {
			const names = []
			if (data.firstname) names.push(data.firstname.trim())
			if (data.otherName) names.push(data.otherName.trim())
			if (data.surname) names.push(data.surname.trim())

			if (names.length == 0) return ""
			return names.join(separator || " / ")
		}

		const ql = s => { return document.querySelectorAll(s) }
		// Function to update the UI with the fetched data
		function updateUIWithData(data, trackingId) {
			//console.log("updateUIWithData: ", data)
			$("#takeOff").text(formatDate(data.dateAndTime))
			$("#from").text(data.from)
			$("#to").text(data.to)
			$("#bkRef").text(trackingId)
			$("#bkId").text(data.ticketId)
			$("#bkStatus").text("OK")

			const createdAt = fbTimeToDate(data.createdAt)
			const dateAndTime = fbTimeToDate(data.dateAndTime)

			var bkDate = formatDate(data.createdAt)
			if (createdAt.getTime() > dateAndTime.getTime()) {
				var bookingDate = new Date()
				bookingDate.setTime(dateAndTime.getTime() - (60 * 60 * 21 * 1000))//21 hrs earlier
				bkDate = formatDate(bookingDate, true)
			}

			$("#bkDate").text(bkDate)
			$("#fullname").text(getFullname(data))

			const checkInTime = fbTimeToDate(data.dateAndTime).getTime() - (30 * 1000)


			$("#checkInIcon").addClass(data.checkInEnabled && Date.now() >= checkInTime ? "fa-print" : "fa-print")

			if (data.checkInEnabled && Date.now() >= checkInTime) {
				$("#checkInStatus").text("CHECKED IN")
				$("#checkInButton").addClass("d-none")

			} else {
				$("#checkInStatus").text("NOT CHECKED IN")
			}

			const waterMark = document.getElementById('water_mark');
			if (data.is_freemium) {
				waterMark.style.display = "block"

			} else {
				waterMark.style.display = "none"
			}

			$('#flightInfoModal').modal('show');
		}

		function subscribeUs(e) {
			e.preventDefault();

			// Get the email input value
			const emailInput = document.querySelector('#sub_email');
			const email = emailInput.value;

			// Validate email
			if (!email || !isValidEmail(email)) {
				Swal.fire({
					icon: 'error',
					title: 'Validation Error',
					text: 'Please enter a valid email address.',
				});
				return;
			}

			// Disable form elements during submission
			document.getElementById('button-addon2').disabled = true;
			document.getElementById('button-addon2').style.cursor = 'not-allowed';
			document.getElementById('button-addon2').style.fontStyle = 'italic';
			document.getElementById('button-addon2').value = 'Subscribing...';

			// Simulate asynchronous subscription submission
			setTimeout(function () {
				// Enable form elements after submission timeout
				document.getElementById('button-addon2').disabled = false;
				document.getElementById('button-addon2').style.cursor = 'pointer';
				document.getElementById('button-addon2').style.fontStyle = 'normal';
				document.getElementById('button-addon2').value = 'Subscribe';

				// Show success message using SweetAlert
				Swal.fire({
					icon: 'success',
					title: 'Success',
					text: 'Thank you for subscribing! You will receive our latest updates.',
				});

				// Clear the email input
				emailInput.value = '';
			}, getRandomTimeout());
		}

		function contactUs(e) {
			e.preventDefault();

			// Reset error messages
			document.getElementById('firstNameError').classList.remove('d-block');
			document.getElementById('lastNameError').classList.remove('d-block');
			document.getElementById('emailError').classList.remove('d-block');
			document.getElementById('messageError').classList.remove('d-block');

			// Get form elements
			var firstName = document.getElementById('firstName').value;
			var lastName = document.getElementById('lastName').value;
			var email = document.getElementById('email').value;
			var message = document.getElementById('message').value;

			var hasError = false;

			// Validate inputs
			if (!firstName || firstName.length == 0) {
				document.getElementById('firstNameError').classList.add('d-block');
				document.getElementById('firstNameError').textContent = 'First name is required.';
				hasError = true;
			}

			if (!lastName) {
				document.getElementById('lastNameError').classList.add('d-block');
				document.getElementById('lastNameError').textContent = 'Last name is required.';
				hasError = true;
			}

			if (!email) {
				document.getElementById('emailError').classList.add('d-block');
				document.getElementById('emailError').textContent = 'Email address is required.';
				hasError = true;

			} else if (!isValidEmail(email)) {
				document.getElementById('emailError').classList.add('d-block');
				document.getElementById('emailError').textContent = 'Please enter a valid email address.';
				hasError = true;

			}

			if (!message) {
				document.getElementById('messageError').classList.add('d-block');
				document.getElementById('messageError').textContent = 'Message is required.';
				hasError = true;
			}

			if (hasError) {
				Swal.fire({
					icon: 'error',
					title: 'Validation Error',
					text: 'Please fill in all fields.',
				});
				return;
			}

			// Disable form elements during submission
			document.getElementById('submitButton').disabled = true;
			document.getElementById('submitButton').style.cursor = 'not-allowed';
			document.getElementById('submitButton').style.fontStyle = 'italic';
			document.getElementById('submitButton').value = 'Sending...';

			// Simulate asynchronous form submission
			setTimeout(function () {
				// Enable form elements after submission timeout
				document.getElementById('submitButton').disabled = false;
				document.getElementById('submitButton').style.cursor = 'pointer';
				document.getElementById('submitButton').style.fontStyle = 'normal';
				document.getElementById('submitButton').value = 'Send Message';

				// Show success message using SweetAlert
				Swal.fire({
					icon: 'success',
					title: 'Success',
					text: `Your contact message was successfully sent. We will get back to you soon via your email address, ${email}!`,
				});
			}, getRandomTimeout());
		}

		// Function to get a random timeout between 2000 and 4000 milliseconds (2 to 4 seconds)
		function getRandomTimeout() {
			return Math.floor(Math.random() * (4000 - 2000 + 1)) + 2000;
		}
		function isValidEmail(email) {
			// Regular expression for a basic email validation
			const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;

			// Test the email against the regex
			return emailRegex.test(email);
		}
	</script>
